     * sece dining
     
     CODE:
     
     x=str(input())
     y=int(input())
     if(x=='front' and y==1):
         print('Left Handed')
     elif(x=='rear' and y==2):
         print('Left Handed')
     elif(x=='front' and y==2):
         print('Right Handed')
     elif(x=='rear' and y==1):
         print('Right Handed')
         
     * Intersection of circle
     
     CODE:
     
     x1=int(input())
     y1=int(input())
     r1=int(input())
     x2=int(input())
     y2=int(input())
     r2=int(input())
     d=((((x1-x2)**2)+((y1-y2)**2))**(1/2))
     if(r1+r2)==d:
         print('The circles are tangential')
     elif(r1+r2)>d:
         print('The circles overlap')
     else:
         ('The circles do not overlap')
         
     * Microwave oven
     
     CODE:
     
     n=int(input("Enter the number of items \n"))
     t=float(input("Enter the single item heating time \n"))
     if (n==1):
         print("The recommended heating time is","%.2f" %t)
     elif(n==2):
         print("The recommended heating time is","%.2f" %(1.5*t))
     elif(n==3):
         print("The recommended heating time is","%.2f" %(2*t))
     else:
         print("number of items is more")
         
         
     *Ameoba multiplication
     
     CODE:
     
     i=int(input("Enter the number of months \n"))
     a=0
     b=1
     count=2
     while count<i:
         c=a+b
         a=b
         b=c
         count=count+1
     print("The amoeba size is",c)
     
     
    * sum of digits
    
    CODE:
    
      n=int(input())
      sum=0
      while(n>0):
          rem=n%10
          sum=sum+rem
          n=n//10
      print(sum)
      
     * Friendly pair
     
     CODE:
     
     x=int(input())
     y=int(input())
     sum1=0
     sum2=0
     for i in range(1,x):
         if(x%i==0):
           sum1+=i
           print(sum1)
     for j in range(1,y):
         i     f(y%j==0):
           sum2+=j
           print(sum2)
     if(sum1/x)==(sum2/y):
         print("Friendly pair")
     else: 
         print("Not Friendly pair")
         
     * Harshard number
     
     CODE:
     
     n=int(input())
     sum=0
     while(n>0):
        b=n%10
        sum=sum+b
        n=n//10
     if(n%sum!=0):
        print("Harshad Number")
     else:
        print("Not Harshad Number")
        
     *LCM of two numbers
     
     CODE:
     
     n1=int(input())
     n2=int(input())
     sum=1
     if(n1>n2):
         x=n1
     else:
         x=n2
     while x!=0:
         if x%n1==0 and x%n2==0:
             print("lcm of",n1,"and",n2,"is",x)
             break
         x=x+1
            
     *Palindrome of a number
     
     CODE:
     
     a=int(input())
     sum==0
     rev=0
     while(a>0):
        b=a%10
        rev=rev*10+b
        a=a//10
        print(rev)
        print(a)
     if sum==rev:
         print("it is a palindrome")
     else:
         print("it is not a palindrome")
         
      *Perfect number
      
      CODE:
      
      a=int(input())
      sum=0
      for i in range(1,a):
          if (a%i)==0:
              sum+=i
              print(sum)
      if (sum)==a:
              print("perfect number")
      else:
              print("not a perfect number")
              
      *Prime number
      
      CODE:
      
      num=int(input())
      if num>1:      
          for i in range (2,int(num/2)+1):
              if(num%i)==0:
                print(num," not a prime ")
                break
          else:
              print(num, "is a prime number")
      else:
          print(num,"is not a prime number")
          
       *Prime numbers in a range
       
       CODE:
       
       a=int(input())
       b=int(input())
       for i in range(a,b+1):
           if i>1:
               for j in range(2,i):
                    if((i%j)==0):
                      break
               else:
                   print(i)
                   
       *Reverse of a number
       
       CODE:
       
       a=int(input())
       rev=0
       while(a>0):
           b=a%10
           rev=rev*10+b
           a=a//10
       print(rev)
       
      *Strong number 
      
      CODE:
      
     n=int(input())
     sum=0
     temp=n
     while(n):
         i=1
         fact=1
         rem=n%10
         while(i<=rem):
             fact=fact+i
             i=i+1
         sum=sum+fact
         n=n//10
     if(sum==temp):
         print("Strong Number")
     else:
         print("Not a Strong Number")
         
         
     * Sum of natural numbers
     
     CODE:
     
    n=int(input())
    sum=0
    while(n>0):
        sum=sum+n
        n=n-1
    print(sum)
    
    *Sum of numbers in range
    
    CODE:
    
    x=int(input())
    y=int(input())
    sum=0
    for i in range(x,y+1):
        sum=sum+i
    print(sum)
    
  * Series 1
  
  CODE:
  
  n=int(input())
  for i in range(1,n+1):
      print(i**2)  
      
      
  *series 2
  
  CODE:
  
  n=int(input())
  a=6
  print(a)
  for i in range (1,n):
      a=a+i*5
      print(a)
      
      
   *Series 3
   
   CODE:
   
   n=int(input())
   for i in range(1,n+1):
        print(3**i)



      
     
     
         
         
           


    


     
     






     
     

       
    

  
