     print(f"Binoy's house is located at {(x1+x2)/2,(y1+y2)/2}" )
     
     *greatest of two numbers
     
     CODE:
     
     a=int(input())
     b=int(input())
     if a==b:
        print(f'{a} is equal to {b}')
     elif a>=b:
        print(f'{a} is greater than {b}')
     else :
        print(f'{a} is less than {b}')
        
      *vowels or consonants
      
      CODE:
      
      a=input()
      vowel="aeiouAEIOU"
      if a>='a'and a<='z' or a>='A' and a<='Z':
          if a in vowel:
              print("vowel")
          else:
              print("consonant")
      else:
          print("not an alphabet")
          
          
       * grading system
       
       CODE:
       
        a=int(input())
        if a==100:
            print("s")
        elif a<100 and a>=90:
            print("A")
        elif a<90 and a>=80:
            p      rint("B")
        elif a<80 and a>=70:
            print("C")
        elif a<70 and a>=60:
            print("D")
        elif a<60 and a>=50:
            print("E")
        elif a<50:
            print("F")
        else:
            print("invalid input")
            
      *profit or loss
      
      CODE:
      
      cp=float(input("Enter the price of a dozen mangoes\n"))
      mango=float(input("Enter the price at which 1 mango is being sold\n"))
      sp=12*mango
      if sp>cp:
          p=str(int(sp-cp))+".00"
          print(f"Profit:Rs.{p}")
      elif sp<cp:
          l=str(int(cp-sp))+".00"
          print(f"Loss:Rs.{l}")
      else:
          print("no profit nor loss")
          
      *fee collection
      
      CODE:
      
      print("Enter the student type")
X=str(input())
if(X=="MSDS"):
    TF=float(input('Enter tuition fee\n'))
    BF=float(input('Enter bus fee\n'))
    print("The fees to be paid by the student is Rs.","%.2f" %(TF+BF))
elif(X=="MSH"):
    TF=float(input("Enter tuition fee\n"))
    HF=float(input("Enter hostel fee\n"))
    print("The fees to be paid by the student is Rs.","%.2f" %(TF+HF))
elif(X=="MGSDS"):
    TF=float(input("Enter tuition fee\n"))
    BF=float(input("Enter bus fee\n"))  
    print("The fees to be paid by the student is Rs.","%.2f" %((1.5*TF)+BF))
elif(X=="MGSH"):    
    TF=float(input("Enter tuition fee\n"))
    HF=float(input("Enter hostel fee\n"))
    print("The fees to be paid by the student is Rs.","%.2f" %((1.5*TF)+HF))
    
  *y2k problem detector
  
  CODE:
  
  
    a=int(input("Enter Year of Birth\n"))
    b=int(input("Enter Current year\n"))
    if a<b:
        print(f'Your age is {b-a}')
    elif a>b:
        print(f'Your age is {100-a+b}') 
        
   * lab allocation
    
    CODE:
    
  L1=int(input("Enter x\n"))
  L2=int(input("Enter y\n"))
  L3=int(input("Enter z\n"))
  if L1<L2 and L1<L3:
      print("L1 has the minimal seating capacity")
  elif L2<L1 and L2<L3:
      print("L2 has the minimal seating capacity")
  else:
      print("L3 has the minimal seating capacity")
      
   * gardening
   
   CODE:
   
   r=int(input())
   c=int(input())
   n=int(input())
   if (n%c==0 or n%c==1 or n>=1 and n<=c):
       print("Yes")
   else:
       print("No")
       
    * hotel tariff calculator
    
    CODE:
    
    n=int(input())
    r=float(input())
    d=int(input())
    if n>=4 and n<=6 or n>=11 and n<=12:
        t="%.2f" %((1.2*r)*d)
        print(f"Hotel Tariff:Rs.{t}")
    elif n<=12:
        t="%.2f" %(r*d)
        print(f"Hotel Tariff:Rs.{t}")
    else:
        print("invalid input")
        
     * RGY Lights
      
